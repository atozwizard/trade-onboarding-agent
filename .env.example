# Environment Variables for TradeOnboarding Agent

# Upstage Solar API
UPSTAGE_API_KEY=your_Upstage_API_key_here

# LangSmith (for tracing and monitoring)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=trade-onboarding-agent
LANGSMITH_TRACING=true

# Database (optional for MVP)
DATABASE_URL=

# Redis Session Store (for production)
# Option 1: Use REDIS_URL (recommended for cloud services like Redis Cloud, AWS ElastiCache)
REDIS_URL=
# Option 2: Use individual connection parameters (for local/self-hosted Redis)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_SSL=false
SESSION_TTL=3600  # Session expiration time in seconds (1 hour)
USE_REDIS_SESSION=false  # Set to 'true' for production, 'false' for development

# Application Settings
ENVIRONMENT=development
DEBUG=true

# Vector Database Settings
VECTOR_DB_DIR=backend/vectorstore
COLLECTION_NAME=trade_coaching_knowledge
AUTO_INGEST_ON_STARTUP=true  # 서버 시작 시 자동으로 데이터 임베딩 여부 (true/false)
